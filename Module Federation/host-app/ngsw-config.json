{
    "index": "/index.html",
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": [
            "/favicon.ico",
            "/index.html",
            "/*.css",
            "/*.js"
          ]
        }
      }
    ],
    "dataGroups": [
      {
        "name": "api",
        "urls": [
          "/getConfig"
        ],
        "cacheConfig": {
          "strategy": "freshness",
          "maxSize": 100,
          "maxAge": "1h",
          "timeout": "10s"
        }
      }
    ]
  }
  